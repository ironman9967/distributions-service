{
	"name": "distributions-service",
	"version": "0.2.0",
	"bin": "dist/index.js",
	"scripts": {
		"build": "rm -rf dist && npx babel src -d dist && cp package.json dist",
		"index": "node dist/index.js",
		"start": "npm run build && npm run index -- $1",
		"start-service": "npm run build && npm run index -- serve",
		"watch": "npm-watch start-service",
		"publish-patch": "npm run build && npm version patch && npm publish && git push"
	},
	"keywords": [],
	"author": "Tyson Hester",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.5.0",
		"@babel/core": "^7.5.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.5.4",
		"@babel/plugin-transform-async-to-generator": "^7.5.0",
		"@babel/preset-env": "^7.5.4",
		"npm-watch": "^0.6.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ironman9967/distribution-service.git"
	},
	"bugs": {
		"url": "https://github.com/ironman9967/distribution-service/issues"
	},
	"homepage": "https://github.com/ironman9967/distribution-service#readme",
	"description": "",
	"babel": {
		"presets": [
			"@babel/preset-env"
		],
		"plugins": [
			"@babel/plugin-proposal-object-rest-spread",
			"@babel/plugin-transform-async-to-generator"
		]
	},
	"watch": {
		"start-service": {
			"patterns": [
				"src"
			],
			"extensions": "js",
			"quite": true
		}
	},
	"dependencies": {
		"@babel/polyfill": "^7.4.4",
		"@hapi/good": "^8.2.0",
		"@hapi/good-console": "^8.1.0",
		"@hapi/good-squeeze": "^5.2.0",
		"@hapi/hapi": "^18.3.1",
		"@tweenjs/tween.js": "^17.4.0",
		"cli": "^1.0.1",
		"lodash": "^4.17.15",
		"socket.io": "^2.2.0"
	}
}
